<div class="form">
    <form action="/signup" method="post" enctype="multipart/form-data">
        <dl>
            <dt>Login:</dt>
            <dd><input type="text" name="login"></dd>
            <dt>Pass:</dt>
            <dd><input type="password" name="pass"></dd>
            <dt>Confirm pass:</dt>
            <dd><input type="password" name="cpass"></dd>
            <dt>Image:</dt>
            <dd><input type="file" name="image" accept="image/*"></dd>
            <dd><input type="submit"></dd>
        </dl>
    </form>
</div>